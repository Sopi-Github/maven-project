pipeline
  { 
    agent any 
	stages
	{
	    stage ('scm checkout')
		{
		  steps {
			git branch: 'master', url: 'https://github.com/Sopi-Github/maven-project.git'
		       }
		}
		stage('parallel deployment'){
		   parallel {
		   stage('compile')
		     {
			   steps {
			      sh 'mvn copmile'
				     }
			  }
		    stage('package')
			  {
			   steps {
			      sh 'mvn package'
				  }
			 }
			 }
			 failFast true
			 }
			 }
			 }
